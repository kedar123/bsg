              "   $<%=a.competitions_user.send(price_array[image_array.index(a.image_name)]);%>";
            ss<%=cu.competition_id%>.add_slide(s);
        <%end%>       
      <%end%> 		
    <%end%>
      for (var i=0; i < ss<%=cu.competition_id%>.slides.length; i++) {
